{
  "manifest_version": 3,
  "name": "PR Review Tracker",
  "version": "1.4.1",
  "description": "Track remaining lines to review as you check off files in GitHub PRs",
  "permissions": [
    "storage"
  ],
  "content_scripts": [
    {
      "js": ["content.js"],
      "css": ["styles.css"],
      "run_at": "document_end"
    }
  ],
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  }
}
